{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "target": "es5",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2015", "dom"]
  },
  "exclude": ["docs", "dist", "node_modules"]
}
